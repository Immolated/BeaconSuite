<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pl.psnc.labiot.beaconsuite">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_suite"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_suite"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".App">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EstimoteLegacySDK_BeaconScanning"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".GoogleCloud_BeaconRegistration"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".EstimoteLegacySDK_BeaconConfiguration"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".GoogleCloud_BeaconUseCase"
            android:parentActivityName=".MainActivity"/>
    </application>

</manifest>